<% if well.images.attached? %>
  <div class="d-flex flex-wrap gap-3 justify-content-center">
    <% well.images.each do |image| %>
      <%= image_tag image.variant(resize_to_limit: [400, 400]), class: "img-thumbnail" %>
    <% end %>
  </div>
<% else %>
  <p>No images attached to this well.</p>
<% end %>